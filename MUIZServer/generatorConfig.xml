<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<context id="mysqlTables" targetRuntime="MyBatis3">
		<commentGenerator>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- 配置数据库连接 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/blog" userId="root"
			password="123456">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- 配置java Model生成 -->
		<!-- <javaModelGenerator targetPackage="com.hsp.article.pojo" targetProject=".\src"> 
			<property name="enableSubPackages" value="true" /> <property name="trimStrings" 
			value="true" /> </javaModelGenerator> 配置mapper.xml文件生成 <sqlMapGenerator targetPackage="com.hsp.article.mapper" 
			targetProject=".\src"> <property name="enableSubPackages" value="true" /> 
			</sqlMapGenerator> 配置Mapper.java 接口生成 <javaClientGenerator type="XMLMAPPER" 
			targetPackage="com.hsp.article.mapper" targetProject=".\src"> <property name="enableSubPackages" 
			value="true" /> </javaClientGenerator> -->

		<!-- 配置java Model生成 -->
		<!-- <javaModelGenerator targetPackage="com.hsp.navigation.pojo" targetProject=".\src"> 
			<property name="enableSubPackages" value="true" /> <property name="trimStrings" 
			value="true" /> </javaModelGenerator> 配置mapper.xml文件生成 <sqlMapGenerator targetPackage="com.hsp.navigation.mapper" 
			targetProject=".\src"> <property name="enableSubPackages" value="true" /> 
			</sqlMapGenerator> 配置Mapper.java 接口生成 <javaClientGenerator type="XMLMAPPER" 
			targetPackage="com.hsp.navigation.mapper" targetProject=".\src"> <property 
			name="enableSubPackages" value="true" /> </javaClientGenerator> -->
		<!-- 配置java Model生成 -->
		<javaModelGenerator targetPackage="com.hsp.blog.pojo"
			targetProject=".\src">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- 配置mapper.xml文件生成 -->
		<sqlMapGenerator targetPackage="com.hsp.blog.mapper"
			targetProject=".\src">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!-- 配置Mapper.java 接口生成 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.hsp.blog.mapper" targetProject=".\src">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!-- <table tableName="invotation"></table> -->
		<!-- <table tableName="resource"></table> <table tableName="user"></table> 
			<table tableName="userinfo"></table> <table tableName="role_reslink"></table> 
			<table tableName="user_rolelink"></table> -->
		<!-- <table tableName="article"></table> -->

		<!-- <table tableName="article"> <columnOverride column="*" javaType="VARCHAR" 
			/> </table> -->
		<!-- <table tableName="navigation"> <columnOverride column="*" javaType="VARCHAR" 
			/> </table> -->
		<table tableName="message">
			<columnOverride column="*" javaType="VARCHAR" />
		</table>
	</context>
</generatorConfiguration>








